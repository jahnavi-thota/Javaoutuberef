class Emp
{
	int eid;
	int salary;
	static String ceo;
	static {
		ceo="Janu";
		}	
	public void show() {
		System.out.println(eid+ " : "+salary+" : " +ceo);
	}
}
public class StaticDemo {

	public static void main(String[] args) {
		Emp navin =  new Emp();
		navin.eid=9;
		navin.salary=25000;
		//Emp.ceo="Jahnavi";
		Emp rahul = new Emp();
		rahul.eid=25;
		rahul.salary=50000;
		//Emp.ceo="Jahnavi";
		//Emp.ceo="Krish";
		navin.show();
		rahul.show();
		
	}

}
